DELIMITER //
CREATE PROCEDURE proc()
      begin
      declare a int;
declare b int;
      set a=1;
      set b=1;
while 1 do
    set a=1;
    while a<6 do
        set b=1;
        while b<4 do
            insert into biao1(c,d) values(concat("a",a),concat("b",b));
            set b=b+1;
        end while;
        set a=a+1;
    end while;
end while;
end;
//
DELIMITER ;
